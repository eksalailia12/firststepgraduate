<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#50B0F9C5"
    android:padding="20dp"
    android:layout_width="300dp"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/menu_jadwal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="20dp"
        android:padding="20dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        android:text="Jadwal Seminar Proposal"
        android:background="#EFEC9C"/>
    <TextView
        android:id="@+id/menu_daftar"

        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="20dp"
        android:padding="20dp"
        app:layout_constraintTop_toBottomOf="@id/menu_jadwal"
        android:text="Daftar Dosen Penguji"
        android:background="#EFEC9C"/>
    <TextView
        android:id="@+id/menu_alur"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="20dp"
        android:padding="20dp"
        app:layout_constraintTop_toBottomOf="@id/menu_daftar"
        android:text="Alur Sempro dan Sidang"
        android:background="#EFEC9C"/>

</androidx.constraintlayout.widget.ConstraintLayout>